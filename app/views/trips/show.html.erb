<h1><%=@trip.name%></h1>
<div id="dates">
  <%=display_dates(@trip)%>
</div>
<%=render 'edit_time'%><br>


<h2> Destinations!</h2>

<%trip_destinations(@trip).each do |dest|%>
<li><%=link_to dest.name, destination_path(dest)%></li>
<%end%>

<%=link_to "Add New Destination", new_trip_destination_path(@trip)%>
